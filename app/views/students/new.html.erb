<h1>New Student</h1>
<div class="row">
  <div class="col-md-6 col-md-offset-3">
<%= simple_form_for(@student) do |f| %>
  
  <%= f.input :StudentNumber %>
  <%= f.input :FirstName %>
  <%= f.input :LastName %>
  <%= f.input :Email %>
  <%= f.input :Password %>
   <%= f.input :CourseID %>

  <%= f.button :submit%>
<% end %>
  </div>
</div>